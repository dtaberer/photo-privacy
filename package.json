{
  "name": "photo-privacy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/fetch-face-models.mjs && node scripts/prepare-ort-sidecars.mjs",
    "ort:prep": "node scripts/prepare-ort-sidecars.mjs",
    "dev:clean": "rimraf node_modules/.vite && npm run ort:prep && vite --force",
    "dev:reset": "npm run ort:prep && npm run clean:vite && vite --force",
    "dev": "vite",
    "lint": "eslint . --max-warnings=0",
    "preview:serve": "vite preview --port 4173",
    "preview": "vite preview",
    "models:fetch": "node scripts/fetch-face-models.mjs",
    "typecheck": "tsc --noEmit",
    "clean:vite": "node -e \"try{require('fs').rmSync('node_modules/.vite',{recursive:true,force:true})}catch(e){}\"",
    "build": "vite build",
    "build:prod": "vite build",
    "build:verify": "node scripts/verify-ort-dist.mjs",
    "prod:build": "npm run build:prod && npm run build:verify",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@vladmandic/face-api": "^1.7.15",
    "bootstrap": "^5.3.7",
    "exifr": "^7.1.3",
    "face-api.js": "^0.22.2",
    "onnxruntime-web": "^1.17.1",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__react": "^10.0.1",
    "@types/testing-library__user-event": "^4.1.1",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
